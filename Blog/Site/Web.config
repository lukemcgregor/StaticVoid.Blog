	<connectionStrings>
		<add name="StaticVoid.Blog" providerName="System.Data.SqlClient" connectionString="Data Source=.;Initial Catalog=StaticVoid.Blog;Integrated Security=SSPI;MultipleActiveResultSets=True" />
	</connectionStrings>
	<appSettings>
		<add key="aspnet:UseTaskFriendlySynchronizationContext" value="true" />
		<add key="webpages:Version" value="2.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="PreserveLoginUrl" value="true" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />
		
		<!--=local dev key for local.blog.staticvoid.co.nz, needs replacing in prod-->
